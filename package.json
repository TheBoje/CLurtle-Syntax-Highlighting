{
	"name": "clurtle-syntax-highlighting",
	"displayName": "CLurtle Syntax Highlighting",
	"description": "provide syntax highlighting for CL files",
	"publisher": "TheBoJe",
	"author": {
		"name": "TheBoJe",
		"url": "https://github.com/TheBoje"
	},
	"repository": {
		"url": "https://github.com/TheBoje/CLurtle-Syntax-Highlighting"
	},
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "clurtle",
				"aliases": [
					"CLurtle",
					"clurtle"
				],
				"extensions": [
					".cl"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "clurtle",
				"scopeName": "source.cl",
				"path": "./syntaxes/clurtle.tmLanguage.json"
			}
		],
        "snippets": [
            {
                "language": "clurtle",
                "path": "./snippets.json"
            }
        ]
	},
